    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
          // Cerrar la instancia actual de Paso_2
    this.dispose();

    // Crear una instancia de Paso_1 y hacerla visible
    Paso_2 pantalla2 = new Paso_2();
    pantalla2.setVisible(true);
    }                                        

    public void Registrar(String args[]) {
