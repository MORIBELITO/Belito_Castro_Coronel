    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         // Realizar acciones para el pedido urgente
        // ...

        // Marcar que se ha realizado un pedido
        pedidoRealizado = true;
        pedidoNormalElegido = false;
        pedidoEspecialElegido = true;
    }                                        

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // Realizar acciones para el pedido normal
    // ...
    // Marcar que se ha realizado un pedido
    pedidoRealizado = true; 
    pedidoNormalElegido = true;
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
         // Verificar si se ha realizado un pedido antes de abrir la siguiente pantalla
        if (pedidoRealizado) {
            // Cerrar la instancia actual de Paso_1
            this.dispose();

            // Crear una instancia de Paso_2 y hacerla visible
            Paso_2 pantalla2 = new Paso_2();
            pantalla2.setVisible(true);
        } else {
            // Mostrar un mensaje o realizar alguna acción indicando que se debe realizar un pedido primero
            // ...
        }
    
    }                                        

